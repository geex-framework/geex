{
  "name": "@geexbox/gqlgen",
  "version": "1.0.0",
  "description": "Vite plugin to generate typed GraphQL operations near files via GraphQL Code Generator",
  "keywords": ["geex", "vite", "plugin", "graphql", "codegen", "typed-document-node"],
  "license": "MIT",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "engines": {
    "node": ">=18.20.0"
  },
  "peerDependencies": {
    "vite": "^4.0.0 || ^5.0.0",
    "@graphql-codegen/near-operation-file-preset": "^3.1.0",
    "@graphql-codegen/typed-document-node": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.0",
    "@graphql-codegen/typescript-apollo-client-helpers": "^3.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "graphql": "^16.11.0",
    "graphql-tag": "^2.12.6"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc -p tsconfig.json",
    "prepare": "pnpm run build",
    "prepublishOnly": "pnpm run build",
    "test": "pnpm run build && pnpm -C test build",
    "release": "pnpm publish --access public --registry https://registry.npmjs.org/"
  },
  "dependencies": {
    "@graphql-codegen/cli": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.0"
  }
}
