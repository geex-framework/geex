  "scripts": {
    "ng": "ng",
    "gqlgen": "graphql-codegen --config .graphqlrc.yml",
    "start": "ng s --port=4201 --host 0.0.0.0 --disable-host-check",
    "build-production": "npm run ng-high-memory build -- --configuration production",
    "build-dev": "npm run ng-high-memory build -- --configuration dev",
    "build-test": "npm run ng-high-memory build -- --configuration test",
    "build-RongxinTesting": "npm run ng-high-memory build -- --configuration RongxinTesting",
    "build-RongxinProduction": "npm run ng-high-memory build -- --configuration RongxinProduction",
    "build-x_Org_xDemo": "npm run ng-high-memory build -- --configuration x_Org_xDemo",
    "test": "ng test",
    "lint": "npm run lint:ts && npm run lint:style",
    "e2e": "ng e2e",
    "ng-high-memory": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng",
    "hmr": "ng s -o --hmr",
    "analyze": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --stats-json",
    "analyze:view": "source-map-explorer dist/**/*.js",
    "analyze:report": "webpack-bundle-analyzer dist/stats.json",
    "test-coverage": "ng test --code-coverage --watch=false",
    "color-less": "ng-alain-plugin-theme -t=colorLess",
    "theme": "ng-alain-plugin-theme -t=themeCss",
    "icon": "ng g ng-alain:plugin icon",
    "lint:ts": "ng lint --fix",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less --fix",
    "postinstall": "yarn patch-package && cd .. && cd .. && cd .. && cd .. && powershell husky install client/x_proj_x/x_Proj_x.Client/x_Proj_xClientApp/.husky",
    "pregqlgen": "set NODE_TLS_REJECT_UNAUTHORIZED='0'"
