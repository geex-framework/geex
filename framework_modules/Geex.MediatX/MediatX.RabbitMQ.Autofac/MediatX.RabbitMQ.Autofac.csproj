<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <Authors>Lulus,Geex</Authors>
        <Description>RPC extensions for mediatr via pipeline</Description>
        <Copyright>Copyright Lulus</Copyright>
        <Features>strict</Features>
        <PackageTags>mediator;request;response;queries;commands;notifications;rpc</PackageTags>
        <GenerateDocumentationFile>false</GenerateDocumentationFile>
        <MinVerTagPrefix>v</MinVerTagPrefix>
        <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
        <PublishRepositoryUrl>true</PublishRepositoryUrl>
        <IncludeSymbols>true</IncludeSymbols>
        <SymbolPackageFormat>snupkg</SymbolPackageFormat>
        <EmbedUntrackedSources>true</EmbedUntrackedSources>
        <Deterministic>true</Deterministic>
        <TargetFrameworks>net6.0</TargetFrameworks>
        <RepositoryUrl>https://github.com/geex-framework/mediatx</RepositoryUrl>
    </PropertyGroup>


    <ItemGroup>
        <PackageReference Include="MediatR" Version="12.*" />
        <PackageReference Include="Autofac" Version="7.*" />
        <PackageReference Include="RabbitMQ.Client" Version="6.5.*" />
    </ItemGroup>
    <ItemGroup>
      <Compile Update="Extensions\Extensions.cs">
        <Link>Extensions\Extensions.cs</Link>
      </Compile>
      <Compile Include="..\MediatX.RabbitMQ\consts.cs">
        <Link>consts.cs</Link>
      </Compile>
      <Compile Include="..\MediatX.RabbitMQ\MessageDispatcherOptions.cs">
        <Link>MessageDispatcherOptions.cs</Link>
      </Compile>
    </ItemGroup>
    <ItemGroup>
      <ProjectReference Include="..\MediatX\MediatX.csproj" />
    </ItemGroup>

</Project>
